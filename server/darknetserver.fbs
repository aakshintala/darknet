namespace darknetServer;

table KeyFrame {
	width2:int32;
	height2:int32;
	numChannels:int32;
	widthStep:int32;
	data:[float32];
}

struct bbox {
	x:float32;
	y:float32;
	w:float32;
	h:float32;
}

table DetectedObject {
	box:bbox;
	classes:int32;
	objectness:float32;
	sort_class:int32;
	prob:[float32];
}


table DetectedObjects {
	numObjects:int32;
	objects:[DetectedObject];
}

rpc_service ImageDetection {
	RequestDetection(KeyFrame):DetectedObjects (streaming: "none");
}
