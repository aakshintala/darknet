namespace darknetServer;

table KeyFrame {
	width:int32;
	height:int32;
	numChannels:int32;
	widthStep:int32;
	data:[float32];
}

struct box {
	x:float32;
	y:float32;
	w:float32;
	h:float32;
}

table DetectedObject {
	bbox:box;
	classes:int32;
	objectness:float32;
	sort_class:int32;
	prob:[float32];
}


table DetectedObjects {
	objects:[DetectedObject];
}

rpc_service ImageDetection {
	RequestDetection(KeyFrame):DetectedObjects (streaming: "none");
}
